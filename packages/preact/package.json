{
  "name": "@fireworks-js/preact",
  "version": "2.0.1",
  "type": "module",
  "description": "A simple fireworks library!",
  "homepage": "https://fireworks.js.org",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.es.js",
  "main": "./dist/index.cjs.js",
  "main:umd": "./dist/index.umd.js",
  "files": [
    "dist/index.es.js",
    "dist/index.cjs.js",
    "dist/index.umd.js",
    "dist/index.d.ts"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js",
      "default": "./dist/index.es.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crashmax-dev/fireworks-js.git"
  },
  "keywords": [
    "preact",
    "canvas",
    "fireworks",
    "animation"
  ],
  "author": {
    "name": "Vitalij Ryndin",
    "email": "sys@crashmax.ru",
    "url": "https://crashmax.ru"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/crashmax-dev/fireworks-js/issues"
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && tsc",
    "types": "tsc",
    "preview": "vite preview",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "fireworks-js": "workspace:2.0.0"
  },
  "devDependencies": {
    "@preact/preset-vite": "2.3.0",
    "preact": "10.10.0"
  },
  "peerDependencies": {
    "preact": ">=8.0.0"
  }
}
