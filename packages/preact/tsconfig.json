{
  "extends": "@crashmax/tsconfig",
  "compilerOptions": {
    "noImplicitOverride": false,
    "useDefineForClassFields": false,
    "moduleResolution": "node",
    "jsx": "preserve",
    "jsxFactory": "h",
    "jsxFragmentFactory": "Fragment",
    "outDir": "dist",
    "types": [
      "vite/client"
    ]
  },
  "include": [
    "src/**/*"
  ]
}
